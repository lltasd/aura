# Environment variables for SMS booking on Vercel
# Copy to .env.local in the project root (Vercel: set in Project Settings -> Environment Variables)

# SMSC.ru credentials
SMSC_LOGIN=
SMSC_PASSWORD=
# Optional sender name (approved in SMSC)
SMSC_SENDER=

# App secret for signing verification tokens (use long random string)
APP_SECRET=

# Set to true to skip real SMS sending during local testing
SMSC_MOCK=false
